#include <pic32mx.h>  /* Declarations of system-specific addresses etc */
#include "musicbox.h"
#include "setup.h"
#include "example_songs.h"


tone_info example_twinkle[68];
int twinkle_length;
tone_info example_imperial[130];
int imperial_length;

void setup_songs() {
	
//{0, 10}, 
	int j;
	int l;
	
	tone_info twinkle[68] = {{G4, 2}, {0, 10}, {G4, 2}, {0, 10}, {D5, 2}, {0, 10}, {D5, 2}, {0, 10}, {E5, 2}, {0, 10}, {E5, 2}, {0, 10}, {D5, 1}, {0, 10}, {C5, 2}, {0, 10}, {C5, 2}, {0, 10}, {B4, 2}, {0, 10}, {B4, 2}, {0, 10}, {A4, 2}, {0, 10}, {A4, 2}, {0, 10}, {G4, 1}, {0, 10}, {C5, 2}, {0, 10}, {C5, 2}, {0, 10}, {B4, 2}, {0, 10}, {B4, 2}, {0, 10}, {A4, 2}, {0, 10}, {A4, 2}, {0, 10}, {G4, 1}, {0, 10}, {G4, 2}, {0, 10}, {G4, 2}, {0, 10}, {D5, 2}, {0, 10}, {D5, 2}, {E5, 2}, {0, 10}, {E5, 2}, {0, 10}, {D5, 1}, {0, 10}, {C5, 2}, {0, 10}, {C5, 2}, {0, 10}, {B4, 2}, {0, 10}, {B4, 2}, {0, 10}, {A4, 2}, {0, 10}, {A4, 2}, {0, 10}, {G4, 1}};
	twinkle_length = sizeof(example_twinkle)/sizeof(example_twinkle[0]);
	for (j = 0; j < twinkle_length; j++){
		example_twinkle[j] = twinkle[j];
	}

	tone_info imperial[130] = {{A2, 1}, {0, 10}, {A2, 1}, {0, 10}, {A2, 1}, {0, 10}, {F2, 2}, {0, 10}, {C3, 4}, {0, 10}, {A2, 1}, {0, 10}, {F2, 2}, {0, 10}, {C3, 4}, {0, 10}, {A2, 1}, {A2, 1}/*18*/, {E3, 1}, {0, 10}, {E3, 1}, {0, 10}, {E3, 1}, {0, 10}, {F3, 2}, {0, 10}, {C3, 4}, {0, 10}, {A2, 1}, {0, 10}, {F2, 2}, {0, 10}, {C2, 4}, {0, 10}, {A2, 1}, {A2, 1}/*36*/, {A3, 1}, {0, 10}, {A2, 2}, {0, 10}, {A2, 4}, {0, 10}, {A3, 1}, {0, 10}, {GS3, 2}, {0, 10}, {G3, 4}, {0, 10}/*38*/, {FS3, 4}, {0, 10}, {E3, 4}, {0, 10}, {F3, 2}, {0, 10}, {AS2, 2}, {0, 10}, {DS3, 1}, {0, 10}, {D3, 2}, {0, 10}, {CS3, 2}, {0, 10}/*62*/, {C3, 4}, {0, 10}, {B2, 4}, {0, 10}, {C3, 2}, {0, 10}, {F2, 1}, {0, 10}, {GS2, 2}, {0, 10}, {F2, 2}, {0, 10}, {A2, 4}, {0, 10}, {C3, 1}, {0, 10}, {A2, 2}, {0, 10}, {C3, 4}, {0, 10}, {E3, 1}, {E3, 1}/*84*/, {A3, 1}, {0, 10}, {A2, 2}, {0, 10}, {A2, 4}, {0, 10}, {GS3, 2}, {0, 10}, {G3, 4}, {0, 10}/*92*/, {FS3, 4}, {0, 10}, {E3, 4}, {0, 10}, {F3, 2}, {0, 10}, {AS2, 2}, {0, 10}, {DS3, 1}, {0, 10}, {D3, 2}, {0, 10}, {CS3, 2}, {0, 10}/*106*/, {C3, 4}, {0, 10}, {B2, 4}, {0, 10}, {C3, 2}, {0, 10}, {F2, 1}, {0, 10}, {GS2, 2}, {0, 10}, {F2, 2}, {0, 10}, {A2, 4}, {0, 10}, {A2, 1}, {0, 10}, {F2, 2}, {0, 10}, {C3, 4}, {0, 10}, {A2, 1}, {A2, 1}, {A2, 1}, {A2, 1}/*130*/};
	imperial_length = sizeof(example_imperial)/sizeof(example_imperial[0]);
	for (l = 0; l < imperial_length; l++){
		example_imperial[l] = imperial[l];
	}

}
